@template linkSource(cita)@
<span class="source">
    <aside class="citation">
        @nu.mine.mosher.Util.styleCitation(cita)@
        @include attachments(nu.mine.mosher.Util.asList(cita.getAttachments()))@
    </aside>
    <div class="note">
        @nu.mine.mosher.Util.styleTranscripts(cita)@
    </div>
</span>
