@template index(rGed, root)@

@include header()@

@include head(null, root)@

<body>
    <div class="homePage">
        <p>Please choose a genealogy to explore:</p>

        <dl>
            @loop i : rGed.size()@
                <dd>{<a href="@rGed[i].getName()@/persons/">@rGed[i].getName()@</a>}</dd>
                <dt>@rGed[i].getDescription()@</dt>
            @end loop@
        </dl>
    </div>

    @include foot()@
</body>

@include footer()@
