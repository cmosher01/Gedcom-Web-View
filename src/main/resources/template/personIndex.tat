@template personIndex(people, index, root)@

@include header()@

@include head(null, root)@

<body>
    @include nav(null, null, null)@

    <ul class="nameList">
        @loop i : people.size()@
            <li>
                <a href="@people[i].getUuid()@">
                    @people[i].getNameSortedDisplay()@
                </a>
            </li>
        @end loop@
    </ul>

    @include foot()@
</body>

@include footer()@
