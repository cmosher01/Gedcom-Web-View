@template personIndex(people,index)@

@include header()@

@include head(null)@

<body>
    @include nav(null,null)@

    <ul class="nameList">
        @loop i : people.size()@
            <li>
                <a href="?person_uuid=@people[i].getUuid()@">
                    @people[i].getNameSortedDisplay()@
                </a>
            </li>
        @end loop@
    </ul>

    @include foot()@
</body>

@include footer()@
